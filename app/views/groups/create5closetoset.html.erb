<h1>Listing groups</h1>
<%= @foundgroupssets.count  %>
<table>
  <tr>
    <th>first</th>
    <th>second</th>
    <th>third</th>
    <th>fouth</th>
    
    
    
  </tr>

<% @foundgroupssets.each do |gp| %>
  <tr>
    <%= gp.inspect %>
    <% agp = gp.to_a() %>
    <td><%= agp[0] %></td>
    <td><%= agp[1] %></td>
    <td><%= agp[2] %></td>
    <td><%= agp[3] %></td>
    
    
    
  
  </tr>
<% end %>
</table>

